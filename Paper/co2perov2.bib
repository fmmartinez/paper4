@article{tan_co_2011,
	title = {{CO}\$\{\}\_\{2\}\$ dissociation activated through electron attachment on the reduced rutile {TiO}\$\{\}\_\{2\}\$(110)-1\${\textbackslash}ifmmode{\textbackslash}times{\textbackslash}else{\textbackslash}texttimes{\textbackslash}fi\{\}\$1 surface},
	volume = {84},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.84.155418},
	doi = {10.1103/PhysRevB.84.155418},
	abstract = {Converting CO2 to useful compounds through the solar photocatalytic reduction has been one of the most promising strategies for artificial carbon recycling. The highly relevant photocatalytic substrate for CO2 conversion could be the popular TiO2 surfaces. However, the lack of accurate measurements for the energy level alignment that determines the CO2 reduction on TiO2 has limited our ability to control these complicated photocatalysis processes. We report here a systematic study on the reduction of CO2 at specific sites of the rutile TiO2(110)-1{\texttimes}1 surface using scanning tunneling microscopy at 80 K. The dissociation of CO2 molecules is found to be activated by one electron attachment process with an energy threshold of 1.8 eV above the Fermi level (or 1.4 eV above the TiO2 conduction band onset), while the lowest unoccupied molecular orbital (LUMO) of the adsorbed CO2 is located at 2.3 eV with respect to the Fermi level. The observed dependence of the dissociation rate on the tunneling current suggests that the reduction of CO2 induced by the electron attachment is a single electron process. These practical information can be used to guide the design of effective catalysts for CO2 photoreduction.},
	number = {15},
	urldate = {2019-01-31},
	journal = {Physical Review B},
	author = {Tan, Shijing and Zhao, Yan and Zhao, Jin and Wang, Zhuo and Ma, Chuanxu and Zhao, Aidi and Wang, Bing and Luo, Yi and Yang, Jinlong and Hou, Jianguo},
	month = oct,
	year = {2011},
	pages = {155418},
	file = {Submitted Version:C\:\\Users\\Franz\\Zotero\\storage\\LGQDBVE3\\Tan et al. - 2011 - CO\$ _ 2 \$ dissociation activated through electron.pdf:application/pdf}
}

